{
  "compilerOptions": {
    "outDir": "./dist",  
    "sourceMap": true,   
    "strict": true,     
    "noImplicitReturns": true, 
    "strictNullChecks": true,   
    "module": "commonjs",  
    "target": "es5",     
    "jsx": "react",   
    "baseUrl": ".", 
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    }    
  },
  "include": [
    "./src/**/*"
  ] 
}
